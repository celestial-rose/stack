{
    "compilerOptions": {
        "target": "ES2022",
        "module": "esnext", // Changed from NodeNext
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "preserve",
        "types": [
            "@cloudflare/workers-types"
        ],
        "lib": [
            "DOM",
            "DOM.Iterable",
            "ESNext"
        ],
        "incremental": true
    },
    "exclude": [
        "node_modules"
    ]
}